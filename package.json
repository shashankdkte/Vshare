{
  "name": "youtube_clone",
  "version": "1.0.0",
  "description": "Node and VanillaJs",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src &&  webpack -w",
    "build:assets": "cd src && webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "xcopy /E/I src\\static build\\static && xcopy /E/I src\\views build\\views",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rmdir /s /q build",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js"
  },
  "author": "Shashank Dhakate",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.4",
    "autoprefixer": "^10.4.14",
    "aws-sdk": "^2.1367.0",
    "axios": "^1.3.6",
    "babel-loader": "^9.1.2",
    "body-parser": "^1.20.2",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "get-blob-duration": "^1.2.0",
    "helmet": "^6.1.5",
    "mini-css-extract-plugin": "^2.7.5",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.1.0",
    "node-sass": "^8.0.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "postcss-loader": "^7.2.4",
    "pug": "^3.0.2",
    "sass-loader": "^13.2.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "passport-local-mongoose": "^8.0.0"
  }
}
